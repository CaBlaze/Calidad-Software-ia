{
  "name": "space-invaders-frontend",
  "version": "1.0.0",
  "description": "Frontend del juego Space Invaders con Kaboom.js",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "npx live-server --port=3000 --host=localhost --entry-file=index.html",
    "start": "npx live-server --port=3000 --host=localhost --entry-file=index.html",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "echo 'No build process needed for vanilla JS'"
  },
  "keywords": ["space-invaders", "game", "kaboom", "frontend"],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "kaboom": "^3000.1.17"
  },
  "devDependencies": {
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "live-server": "^1.2.2"
  }
}